<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextAppearance.Dgca.Toggle" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:clickable">false</item>
        <item name="textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="backgroundTint">@color/btn_toggle_selector</item>
        <item name="strokeColor">@color/white</item>
        <item name="strokeWidth">@dimen/mtrl_btn_stroke_size</item>
    </style>

    <style name="TextAppearance.Dgca.CertificateTitle" parent="">
        <item name="android:textColor">@color/grey_100</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="fontFamily">sans</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="TextAppearance.Dgca.CertificateValue" parent="">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_big</item>
        <item name="fontFamily">sans</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:typeface">normal</item>
    </style>

</resources>